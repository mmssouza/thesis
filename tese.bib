@article{bib:pcaSR,
Author = {Super, BJ},
Title = {{Fast correspondence-based system for shape retrieval}},
Journal = {{PATTERN RECOGNITION LETTERS}},
Year = {{2004}},
Volume = {{25}},
Number = {{2}},
Pages = {{217-225}},
Month = {{JAN 19}},
Abstract = {{Several recently published shape retrieval systems have achieved high
   accuracy on a benchmark 1400-shape dataset. However, some of these
   systems have high pairwise shape matching costs due to their use of
   structural matching or flexible correspondence. The purpose of this
   paper is to demonstrate that a relatively simple shape retrieval system
   based on fixed point correspondences can achieve accuracy comparable
   with the two most accurate prior systems, at significantly higher speed.
   High accuracy is achieved by using a stable curve normalization
   procedure and example-based retrieval. High speed is achieved by three
   techniques: single key-point alignment, fixed correspondences, and
   PCA-based dimensionality reduction. The system is completely automatic
   and does not require that the database shapes have class labels. (C)
   2003 Elsevier B.V. All rights reserved.}},
DOI = {{10.1016/j.patrec.2003.10.003}},
ISSN = {{0167-8655}},
Times-Cited = {{13}},
